{
  "name": "log-analysis",
  "version": "0.17.0",
  "description": "log analysis tools",
  "config": {
    "domains": "public,private",
    "http-port": "3000",
    "https-port": "3443",
    "num-workers": "4",
    "token-period": "1h",
    "userlist-path": "./local/user/userlist.json",
    "public-key-path": "./local/cert/public-key.pem",
    "private-key-path": "./local/cert/private-key.pem",
    "certificate-path": "./local/cert/server-cert.pem",
    "log-path": "./local/log",
    "storage-path": "../data",
    "max-view-size-mb": "150",
    "date-format": "([0-9]{4}-[0-9]{2}-[0-9]{2}[T ][0-9]{2}:[0-9]{2}:[0-9]{2}.?[0-9]{0,6}Z?):?[ |]"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "post-install": "npm run build:pdfmake",
    "add:user": "node ./bin/add-localuser.js",
    "init:user": "node ./bin/init-localuser.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p ./tsconfig.server.json && node ./bin/setup.js",
    "build:client": "webpack",
    "build:test": "tsc -p ./tsconfig.test.json && cpx ./src/test/script/* ./dist/test/script && cpx ./src/test/data/* ./dist/test/data",
    "build:pdfmake": "mkdirp ./node_modules/pdfmake/examples/fonts && cpx \"./lib/font/*\" ./node_modules/pdfmake/examples/fonts && cd ./node_modules/pdfmake && npm i && npm run build && gulp buildFonts",
    "clean": "node ./bin/cleaner.js",
    "clean:once": "node ./bin/cleaner.js 0h",
    "start": "node ./bin/www-master",
    "debug:single": "cross-env LOGLEVEL=debug LOGOUT=console node ./bin/www",
    "debug:multi": "cross-env LOGLEVEL=debug node ./bin/www-master",
    "test:web-api": "node ./dist/test/web-api",
    "test": "npm run test:web-api"
  },
  "resolutions": {
    "braces": "2.3.1",
    "glob-parent": "5.1.2",
    "set-value": "4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aumezawa/log-analysis.git"
  },
  "author": "aume",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aumezawa/log-analysis/issues"
  },
  "homepage": "https://github.com/aumezawa/log-analysis#readme",
  "dependencies": {
    "@ts-stack/markdown": "^1.3.0",
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jquery": "^3.5.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.2.1",
    "multer": "^1.4.2",
    "node-pty": "^0.9.0",
    "pdfmake": "^0.1.70",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-bootstrap-icons": "^1.1.0",
    "react-dom": "^16.13.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "tar": "^6.1.11",
    "xterm": "^4.6.0",
    "xterm-addon-fit": "^0.4.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/form-data": "^2.5.0",
    "@types/helmet": "0.0.46",
    "@types/js-cookie": "^2.2.6",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/log4js": "^2.3.5",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.13.34",
    "@types/pdfmake": "^0.1.17",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/serve-favicon": "^2.5.0",
    "@types/socket.io": "^2.1.6",
    "@types/socket.io-client": "^1.4.33",
    "@types/tar": "^4.0.3",
    "@types/xterm": "^3.0.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^3.5.3",
    "fibers": "^5.0.0",
    "form-data": "^3.0.0",
    "js-yaml": "^3.14.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^6.0.1",
    "path-browserify": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "process": "^0.11.10",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "util": "^0.12.4",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2"
  }
}
