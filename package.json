{
  "name": "esxi-log-viewer",
  "version": "0.27.0",
  "description": "viewer for esxi log (vmsupport log bundle)",
  "config": {
    "domains": "public",
    "http_port": "3000",
    "https_port": "3443",
    "server_timeout": "1800000",
    "num_workers": "4",
    "no_auth": "true",
    "token_period": "12h",
    "userlist_path": "./local/user/userlist.json",
    "public_key_path": "./local/cert/public-key.pem",
    "private_key_path": "./local/cert/private-key.pem",
    "certificate_path": "./local/cert/server-cert.pem",
    "log_path": "./local/log",
    "storage_path": "../data",
    "console_user": "root",
    "linux_shell": "sh",
    "max_view_size_mb": "80",
    "date_format": "([0-9]{4}-[0-9]{2}-[0-9]{2}[T ][0-9]{2}:[0-9]{2}:[0-9]{2}.?[0-9]{0,6}Z?):?[ |]"
  },
  "scripts": {
    "add:user": "node ./bin/add-localuser.js",
    "init:user": "node ./bin/init-localuser.js",
    "build": "npm run build:server && npm run build:client",
    "build:all": "npm run build && npm run build:test",
    "build:server": "tsc -p ./tsconfig.server.json && node ./bin/setup.js",
    "build:client": "webpack",
    "build:test": "tsc -p ./tsconfig.test.json && node ./bin/setup-test.js",
    "clean": "node ./bin/cleaner.js",
    "clean:once": "node ./bin/cleaner.js 0h",
    "start": "cross-env STORAGE_PATH=$npm_config_storage_path NO_AUTH=$npm_config_no_auth node ./bin/www-master",
    "debug": "cross-env LOGLEVEL=debug STORAGE_PATH=$npm_config_storage_path NO_AUTH=$npm_config_no_auth node ./bin/www-master",
    "debug:single": "cross-env LOGLEVEL=debug LOGOUT=console STORAGE_PATH=$npm_config_storage_path NO_AUTH=$npm_config_no_auth node ./bin/www",
    "debug:multi": "cross-env LOGLEVEL=debug LOGOUT=console STORAGE_PATH=$npm_config_storage_path NO_AUTH=$npm_config_no_auth node ./bin/www-master",
    "test": "npm run test:web-api",
    "test:web-api": "npm run build:test && node ./dist/test/web-api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aumezawa/log-analysis.git"
  },
  "author": "aume",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aumezawa/log-analysis/issues"
  },
  "homepage": "https://github.com/aumezawa/log-analysis#readme",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "farmhash": "^3.2.2",
    "helmet": "^3.22.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.4.2",
    "multer": "^1.4.2",
    "node-pty": "^0.9.0",
    "pdfmake": "^0.1.69",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.4.1",
    "tar": "^6.1.11",
    "xterm": "^4.6.0"
  },
  "devDependencies": {
    "@ts-stack/markdown": "^1.5.0",
    "@types/adm-zip": "^0.4.34",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/form-data": "^2.5.0",
    "@types/helmet": "0.0.46",
    "@types/js-cookie": "^2.2.6",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/log4js": "^2.3.5",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.13.34",
    "@types/pdfmake": "^0.1.17",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/recharts": "^1.8.23",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/serve-favicon": "^2.5.0",
    "@types/socket.io": "^2.1.6",
    "@types/socket.io-client": "^1.4.33",
    "@types/tar": "^4.0.3",
    "@types/xterm": "^3.0.0",
    "assert": "^2.0.0",
    "axios": "^1.6.8",
    "bootstrap": "^4.6.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.10.0",
    "fibers": "^5.0.0",
    "form-data": "^3.0.0",
    "js-yaml": "^3.14.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^9.0.0",
    "path-browserify": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "process": "^0.11.10",
    "react-bootstrap-icons": "^1.11.3",
    "recharts": "^2.1.9",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "socket.io-client": "^4.1.3",
    "stream-browserify": "^3.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.1",
    "typescript": "^5.4.3",
    "util": "^0.12.4",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "xterm-addon-fit": "^0.4.0"
  }
}
